class C {
public:
	C() { this.m = 1; }
	void set(int x = 42);
	int m;
	const int k = 10;
private:
	int n;
};

C::set(int x) {
	m = x;
	assert(k == 10);
}

class C2 {
public:
	int m;
	C2();
	void set() { this.m = 2; }
};

C2::C2() {
	set();
}

int main()
{
	C f;
	assert(f.k == 10);
	assert(f.m == 1);
	f.set();
	assert(f.m == 42);
	f.n = 42;  // FIXME: should cause a compile error

	C2 c2;
	assert(c2.m == 2);

	return 0;
}
